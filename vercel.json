{
    "version": 2,
    "builds": [
      {
        "src": "src/server/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/server/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }